<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_register"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.chris.loginregister.RegisterActivity">

    <EditText
        android:layout_width="217dp"
        android:layout_height="44dp"
        android:inputType="textPersonName"
        android:text="User Name"
        android:ems="10"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="37dp"
        android:id="@+id/etUsername" />

    <Button
        android:text="Register"
        android:layout_width="233dp"
        android:layout_height="51dp"
        android:id="@+id/bRegister"
        app:layout_constraintLeft_toLeftOf="@+id/activity_register"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/activity_register"
        android:layout_marginTop="177dp"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintRight_toRightOf="@+id/activity_register"
        tools:layout_constraintRight_creator="1" />



</android.support.constraint.ConstraintLayout>
